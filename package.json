{
  "name": "code-pbat",
  "displayName": "PBAT Language Support",
  "publisher": "simonhochrein",
  "description": "",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.99.0"
  },
  "repository": {
    "url": "https://github.com/simonhochrein/code-pbat"
  },
  "categories": [
    "Programming Languages"
  ],
  "icon": "./icons/pbat.png",
